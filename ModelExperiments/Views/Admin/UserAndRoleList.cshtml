@model IEnumerable<ModelExperiments.Models.UserRoleViewModel>

@{
    ViewBag.Title = "UserandRoles";
}

<h2>UserList</h2>
<p>@ViewBag.msg</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DateJoined)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RoleList)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UserName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DateJoined)
           </td>
           <td>
                @foreach (var rl in item.RoleList)
                {
                    @Html.DisplayFor(modelrl => rl)
                }
           </td> 
        </tr>
    }

</table>

